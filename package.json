{
  "name": "graphqlprisma",
  "version": "1.0.0",
  "description": "Minimal node + typescript starter",
  "main": "index.js",
  "scripts": {
    "watch": "tsc --skipLibCheck --watch",
    "dev": "nodemon --quiet --watch dist --exec 'clear;node dist/index.js'",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint '*/**/*.ts' --quiet .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --loglevel silent './**/*.{json,js,ts,tsx}'"
  },
  "keywords": [
    "node",
    "typescript"
  ],
  "author": "argentinux",
  "license": "MIT",
  "devDependencies": {
    "@prisma/cli": "^2.10.2",
    "@types/express": "^4.17.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "3.9"
  },
  "dependencies": {
    "@prisma/client": "^2.10.2",
    "@types/argon2": "^0.15.0",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "loglevel": "^1.6.8",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0"
  }
}
